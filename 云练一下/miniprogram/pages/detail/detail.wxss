.title {
  margin-top: 70rpx;
  padding: 0px 20rpx;
  font-size: 30rpx;
  font-weight: 800
}

.button{
  margin-top: 40rpx;
  color: #fff;
  background-color: #5b9d86;
  font-size: 28rpx;
}

#weishoucang{
  position:fixed;
  width: 100%;
  bottom: 70rpx;
  background-color: #5b9d86;
  color: #fff;
}

#yishoucang{
  position:fixed;
  width: 100%;
  bottom: 70rpx;
  background-color: #ccc;
  color: #fff;
}

.makedown{
  padding:50rpx 20rpx 150rpx;
}

.changet{
  width: 100%;
  display: flex;
  position: fixed;
  bottom: 0;
  justify-content: center;
}

.changet button{
  flex: 1 !important;
  color: #666;
  font-size: 25rpx;
}

page{
  position: relative;
  height: calc(100% - 70rpx);
}

.page{
  height: 100%;
}

.login{
  width: 500rpx;
  height: 300rpx;
  background-color: #fff;
  border-radius: 20rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}

.mask{
  position: absolute;
  top: -10rpx;
  width: 100vw;
  height: calc(100% + 10rpx);
  background-color: rgba(0,0,0,0.3);
}

.login image {
  display: block;
  margin: 40rpx auto;
}

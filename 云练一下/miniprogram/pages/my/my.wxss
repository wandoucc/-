}

.mylist image {
  width: 45rpx;
  height: 45rpx;
  margin-right: 20rpx;
}

.myname {
  display: flex;
  align-items: center
}

.self{
  display: flex;
  padding: 0rpx 30rpx;
  align-items: center;
  border-bottom: 1px solid #ccc;
}

.self image{
  margin: 50rpx 30rpx;
  width: 120rpx;
  height: 120rpx;
}

.shuoming{
